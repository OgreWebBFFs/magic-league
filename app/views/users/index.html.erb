<div class="cycling-background" style="background: url('<%= get_random_bg_image %>') 50% 0% / cover no-repeat;">
</div>
<div class="user-list">
  <h3 class="user-list_title">Users</h3>
    <ul>
      <% @users.each do |user| %>
        <li>
          <%= link_to user.name, user_path(user), :class => "user-list_user-btn" %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
