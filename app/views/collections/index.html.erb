
<div id="user-wrapper" class="container">
<h3 class="color-blocked dark centered">Collections</h3>
<div class="centered content">

<ul>
  <% @collections.each do |collection| %>
    <li>
       <%= link_to collection.user.name, collection_path(collection.id), :class => "btn user" %> 
    </li>
  <% end %>
</ul>
</div>
</div>