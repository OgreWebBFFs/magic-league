
<div id="user-wrapper" class="container">
<h3 class="color-blocked dark centered">Collections</h3>
<div class="centered content">

<ul>
  <% @collections.each do |collection| %>
    <li>
        <button class="user btn"><%= link_to collection.user.name, collection_path(collection.id) %></btn> 
    </li>
  <% end %>
</ul>
</div>
</div>