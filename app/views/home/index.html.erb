<%= react_component("CyclingBackground", {bgPath:get_random_bg_image}) %>
<%= form_with(url: "/", :class =>"rankings__date_form", method: "get", local: true) do %>
  <%= select_date(@match_date, { order: [:month, :year], prefix: :match_date}, { onchange: ("this.form.submit();") } ) %>
<% end %>
<%= react_component("Rankings", {
   rankedPlayers: @ranked_players,
   unrankedPlayers: @unranked_players,
}) %>
