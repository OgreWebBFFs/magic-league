<%= react_component("CyclingBackground", {bgPath:get_random_bg_image}) %>
<% draffle = Draffle.all.first %>
<% if !draffle.nil? %>
  <%= link_to "/draffles", class: "button draffle-view-button" do %>
    View<br /><%= draffle.name %>
  <% end %>
<% end %>
<%= react_component("Rankings", {
  date: @match_date,
  rankedPlayers: @ranked_players,
  unrankedPlayers: @unranked_players,
  eventRankedPlayers: @event_ranked_players,
  eventUnrankedPlayers: @event_unranked_players
}) %>
